{
    "typescript.preferences.quoteStyle": "single",
    "prettier.trailingComma": "all",
    "prettier.tabWidth": 2,
    "prettier.semi": true,
    "prettier.singleQuote": true,
    "prettier.printWidth": 100,
    "cSpell.language": "en-GB,en-US",
    "cSpell.words": [
        "autodocs",
        "bodyparser",
        "codegen",
        "copyfiles",
        "cyrb",
        "dockerhub",
        "docsearch",
        "esbenp",
        "gitmoji",
        "hcaptcha",
        "Intelli",
        "Khaoz",
        "Kurt",
        "Lourens",
        "nmsassistant",
        "nmscd",
        "NMSUD",
        "pkief",
        "seatonjiang",
        "solidjs",
        "tada",
        "TLDR",
        "Topsy",
        "tspaths",
        "typedi",
        "typeof",
        "vitepress",
        "Xata"
    ],
    "cSpell.ignorePaths": [
        "node_modules",
        "**/node_modules",
        "**/node_modules/**",
        ".git",
        "build",
        "coverage",
        "dist",
        "src/api/**",
        "storybook-static"
    ]
}